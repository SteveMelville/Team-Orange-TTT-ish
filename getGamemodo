                    if(width == "" | height =="" | pnts == "")
                    {
                        alert("Please fill in the fields to proceed");
                        return;
                    }
 
                    console.log(name_);                                      
                    conn.setplayer(name_);
                    
                    conn.CreateGame(width, height, pnts, scrabble);
